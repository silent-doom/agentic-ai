      "cell_type": "code",
      "source": [
        "import os\n",
        "import json\n",
        "import re\n",
        "from google.colab import drive, userdata\n",
        "from pydantic import BaseModel, Field\n",
        "from crewai import Agent, Task, Crew\n",
        "from langchain_google_genai import ChatGoogleGenerativeAI\n",
        "from typing import List\n",
        "\n",
        "# =================================================================\n",
        "# 5. TASK DEFINITIONS\n",
        "# =================================================================\n",
        "\n",
        "# Load the data before defining the task\n",
        "transcript_data = load_transcript_data(TRANSCRIPT_DRIVE_PATH)\n",
        "\n",
        "if transcript_data:\n",
        "    # Task 1: Research the Viral Segments\n",
        "    research_task = Task(\n",
        "        description=f\"\"\"\n",
        "        Analyze the following full, structured video transcript data. Your job is to extract\n",
        "        the top 7 most compelling, short (30-60 second) moments. Focus on moments where\n",
        "        a key insight, strong emotion, or surprising fact is revealed.\n",
        "\n",
        "        TRANSCRIPT DATA TO ANALYZE:\n",
        "        ---\n",
        "        {transcript_data}\n",
        "        ---\n",
        "\n",
        "        Output a clear, bulleted list of 7 segment proposals, including the approximate\n",
        "        start/end times (HH:MM:SS) and a brief note on the segment's viral potential.\n",
        "        \"\"\",\n",
        "        expected_output=\"A bulleted list of 7 potential viral segment proposals with start/end times and brief reasoning for each.\",\n",
        "        agent=researcher\n",
        "    )\n",
        "\n",
        "    # Task 2: Finalize the Plan and Structure the Output\n",
        "    planning_task = Task(\n",
        "        description=f\"\"\"\n",
        "        Review the segment proposals provided by the Researcher. Select the best 5 clips,\n",
        "        ensuring they do not overlap in time.\n",
        "\n",
        "        For each of the final 5 clips, determine the most precise start_time and end_time,\n",
        "        create a catchy 'viral_hook', and provide a brief 'reasoning' why you selected it.\n",
        "\n",
        "        The final output MUST strictly adhere to the ViralClipPlan Pydantic JSON schema.\n",
        "        \"\"\",\n",
        "        expected_output=\"A structured JSON object adhering to the ViralClipPlan schema, containing exactly 5 non-overlapping viral clips.\",\n",
        "        agent=planner,\n",
        "        output_pydantic=ViralClipPlan,\n",
        "        output_file='viral_clip_plan.json',\n",
        "        context=[research_task]\n",
        "    )\n",
        "\n",
        "    # =================================================================\n",
        "    # 6. CREW EXECUTION\n",
        "    # =================================================================\n",
        "\n",
        "    # Define the crew with sequential process\n",
        "    video_crew = Crew(\n",
        "        agents=[researcher, planner],\n",
        "        tasks=[research_task, planning_task],\n",
        "        process='sequential',\n",
        "        verbose=True # Changed from 2 to True for boolean compatibility\n",
        "    )\n",
        "\n",
        "    print(\"--- Starting Multi-Agent Planning Crew ---\")\n",
        "\n",
        "    # Kick off the execution\n",
        "    result = video_crew.kickoff()\n",
        "\n",
        "    print(\"\\n\\n################################################\")\n",
        "    print(\"           AGENT PLANNING COMPLETE              \")\n",
        "    print(\"################################################\")\n",
        "    print(f\"Final Plan saved to: viral_clip_plan.json\")\n",
        "    print(\"\\n--- Final Plan Summary ---\")\n",
        "\n",
        "    if hasattr(result, 'pydantic') and result.pydantic:\n",
        "        # Handle both Pydantic v1 and v2 methods\n",
        "        if hasattr(result.pydantic, 'model_dump_json'):\n",
        "            print(result.pydantic.model_dump_json(indent=2))\n",
        "        else:\n",
        "            print(result.pydantic.json(indent=2))\n",
        "    else:\n",
        "        # Fallback to string representation if Pydantic object is not present\n",
        "        print(result)\n",
        "\n",
        "else:\n",
        "    print(\"Crew execution aborted due to missing transcript data.\")"
      ],
