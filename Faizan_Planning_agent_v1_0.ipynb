        "# =================================================================\n",
        "# 2. STRUCTURED OUTPUT SCHEMA (The Planner's Goal)\n",
        "# =================================================================\n",
        "\n",
        "class ViralMoment(BaseModel):\n",
        "    \"\"\"A single identified segment of the video suitable for a short-form clip.\"\"\"\n",
        "    clip_id: int = Field(description=\"Unique ID for the clip, starting from 1.\")\n",
        "    start_time: str = Field(description=\"The exact start time of the moment, format HH:MM:SS.\")\n",
        "    end_time: str = Field(description=\"The exact end time of the moment, format HH:MM:SS.\")\n",
        "    viral_hook: str = Field(description=\"A concise, 10-word phrase capturing the core conflict or takeaway for the first 3 seconds of the clip.\")\n",
        "    reasoning: str = Field(description=\"Brief justification (20 words max) why this segment has high viral potential (e.g., emotional peak, key information density, shocking reveal).\")\n",
        "\n",
        "class ViralClipPlan(BaseModel):\n",
        "    \"\"\"The final plan for generating video clips.\"\"\"\n",
        "    video_title: str = Field(description=\"The original title or main topic of the long-form video.\")\n",
        "    viral_clips: List[ViralMoment] = Field(description=\"A list of 5 high-potential, non-overlapping short video clips.\")\n"
      ],
      "metadata": {
        "id": "97TSxPqvLot0"
      },
      "execution_count": 5,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# =================================================================\n",
        "# 3. UTILITY FUNCTIONS\n",
        "# =================================================================\n",
        "\n",
        "def load_transcript_data(file_path):\n",
        "    \"\"\"Loads the structured transcript JSON from the specified path.\"\"\"\n",
        "    if not os.path.exists(file_path):\n",
        "        print(f\"ğŸ”´ ERROR: Transcript file not found at {file_path}. Please run the transcription pipeline first.\")\n",
        "        # Attempt to mount drive if not already mounted\n",
        "        drive.mount(DRIVE_MOUNT_PATH, force_remount=True)\n",
        "        if not os.path.exists(file_path):\n",
        "            return None\n",
        "\n",
        "    with open(file_path, 'r', encoding='utf-8') as f:\n",
        "        data = json.load(f)\n",
        "\n",
        "    # Format the data into a readable string for the LLM\n",
        "    formatted_data = \"\"\n",
        "    for i, segment in enumerate(data):\n",
        "        formatted_data += (\n",
        "            f\"Segment {i+1} | TIME: {segment['startTime']} - {segment['endTime']} | \"\n",
        "            f\"SUMMARY: {segment['summary']} | CONTENT: {segment['actualContent']}\\n\"\n",
        "        )\n",
        "    return formatted_data"
      ],
      "metadata": {
        "id": "edAKLMkDMOh6"
      },
      "execution_count": 6,
      "outputs": []
